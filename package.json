{
  "name": "fitland-backend",
  "version": "2.2.1",
  "description": "Simple e-commerce backend for FitLand sports store",
  "main": "server.js",
  "type": "commonjs",
  "scripts": {
    "prebuild": "chmod +x ./node_modules/.bin/prisma || true",
    "build": "prisma generate",
    "start": "node api/index.js",
    "dev": "nodemon src/app.js",
    "db:push": "prisma db push",
    "db:seed": "node prisma/seed.js",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force"
  },
  "keywords": [
    "ecommerce",
    "sports",
    "fitness",
    "api",
    "express",
    "prisma",
    "sqlite"
  ],
  "author": "FitLand Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^4.16.2",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.19.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^4.16.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "nodemon": "^3.1.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}
